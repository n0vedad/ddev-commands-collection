{
  "name": "kmi/ddev-commands-collection",
  "description": "Collection of reusable DDEV commands",
  "license": "MIT",
  "type": "composer-plugin",
  "version": "0.3.1",
  "authors": [
    {
      "name": "Konrad Michalik",
      "email": "hello@konradmichalik.eu"
    },
    {
      "name": "Lucas DÃ¤mmig",
      "email": "lucas.daemmig@rasani.de"
    }
  ],
  "config": {
    "vendor-dir": "vendor",
    "bin-dir": "bin",
    "classmap-authoritative": true
  },
  "require": {
    "composer-plugin-api": "^2.0",
    "composer/composer": "^2.0",
    "symfony/filesystem": "*",
    "symfony/yaml": "*",
    "helhum/typo3-console": "^8.2"
  },
  "autoload": {
    "psr-4": {
      "Kmi\\DdevCommandsCollection\\": "src/"
    }
  },
  "extra": {
    "class": "Kmi\\DdevCommandsCollection\\Composer\\Scripts"
  },
  "scripts": {
    "post-install-cmd": [
      "Kmi\\DdevCommandsCollection\\Composer\\Scripts::updateCommands"
    ],
    "post-update-cmd": [
      "Kmi\\DdevCommandsCollection\\Composer\\Scripts::updateCommands"
    ]
  },
  "minimum-stability": "stable"
}